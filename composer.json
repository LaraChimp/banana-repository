{
    "name": "larachimp/mango-repo",
    "description": "Simple repository package for Laravel 5.",
    "keywords": [
        "php",
        "Laravel5",
        "database",
        "repository",
        "pattern"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "^5.8",
        "larachimp/pine-annotations": "0.2.*"
    },
    "require-dev": {
		"orchestra/testbench": "3.8.*",
		"mockery/mockery": "^1.0"
	},
    "autoload": {
        "psr-4": {
            "LaraChimp\\MangoRepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraChimp\\MangoRepo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        },
        "laravel": {
            "providers": [
                "LaraChimp\\MangoRepo\\MangoRepoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
