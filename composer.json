{
    "name": "larachimp/mango-repo",
    "description": "Simple repository package for Laravel 5.",
    "keywords": [
        "php",
        "Laravel5",
        "database",
        "repository",
        "pattern"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "laravel/framework": "~5.0",
        "doctrine/annotations": "~1.4",
        "doctrine/cache": "^1.6"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "LaraChimp\\MangoRepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraChimp\\MangoRepo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
