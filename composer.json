{
    "name": "larachimp/mango-repo",
    "description": "Simple repository package for Laravel.",
    "keywords": ["php", "Laravel", "database", "repository", "pattern"],
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.0",
        "larachimp/pine-annotations": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.4|^9.0"
	},
    "autoload": {
        "psr-4": {
            "LaraChimp\\MangoRepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraChimp\\MangoRepo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "develop"
        },
        "laravel": {
            "providers": [
                "LaraChimp\\MangoRepo\\MangoRepoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
